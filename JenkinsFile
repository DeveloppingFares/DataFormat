pipeline {
    agent {label 'master'}
    options {
        buildDiscarder logRotator(
                    daysToKeepStr: '16',
                    numToKeepStr: '10'
            )
        disableConcurrentBuilds()
    }
    triggers { cron( H * * * * ) }
    stages {
        stage('Hello') {
            steps {
                echo "Hello"
            }
        }
    }
}
